<!DOCTYPE html>
<html>
<head>
  <title>HackOverflow</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if logged_in? %>
    <%= link_to 'Logout', logout_path, method: 'delete' %>
  <%end%>

<%= yield %>

</body>
</html>
